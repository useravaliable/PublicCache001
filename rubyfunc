def jaimi3(str)
  string=""
  sex=rand(97,113)
  str.each_byte{|x| if x.ord!=sex x=x.ord-sex/2+1+x.ord;if x.class == Double x=abs(x)+1; string+=x.chr}
  return string,sex.chr
