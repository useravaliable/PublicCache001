def jiami2(str)
  string=""
  len=str.length
  str1=(0..len).each do {|a| return chr(ord(a)+rand(97))} 
  strarr=str.split("fuhao")
  count=0
  strarr.each{|a| string=string+a+str1[count] if count <= len;if count == len count=0 }
  return string+str1
